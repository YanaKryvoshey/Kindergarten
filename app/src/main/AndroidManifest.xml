<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kindergarten">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kindergarten">
        <activity android:name=".Activitys.WriteMessageActivity"></activity>
        <activity android:name=".Activitys.ShowMessageActivity" />
        <activity android:name=".Activitys.SendMessageActivity" />
        <activity android:name=".Activitys.ChatActivity" />
        <activity android:name=".Activitys.GardenProfileActivity" />
        <activity android:name=".Activitys.ParentOrTeacherActivity" />
        <activity android:name=".Activitys.LoginActivity" />
        <activity android:name=".Activitys.NewTeacherActivity" />
        <activity android:name=".Activitys.NewChildActivity" />
        <activity android:name=".Activitys.NewGardenActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBBUtcn60Uzkl7_xNRogQl8uDmoOznVqm4" />

        <activity android:name=".Activitys.SeeMapActivity" />
        <activity android:name=".Activitys.AddMapActivity" />
        <activity android:name=".Activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>